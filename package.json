  },
  "homepage": "https://github.com/standard-analytics/web-verse",
  "dependencies": {
    "fast-levenshtein": "1.0.7",
    "sbd": "1.0.1",
    "uuid": "2.0.1"
  },
  "devDependencies": {
    "browserify": "11.2.0",
    "jsdom": "6.5.1",
    "mocha": "2.3.3",
